<Application x:Class="RemoteEasySave.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RemoteEasySave"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary x:Name="Dictionary">
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Style/MainDictonnary.xaml" />
                <ResourceDictionary Source="/Style/SettingsDictionary.xaml" />
                <ResourceDictionary Source="/Style/BackupDictionary.xaml"/>
                <ResourceDictionary Source="/Style/DashboardDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
