<UserControl x:Class="EasySaveV2.MVVM.Views.DashboardViews"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EasySaveV2.MVVM.Views"
             xmlns:vm="clr-namespace:EasySaveV2.MVVM.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="344" d:DesignWidth="604">
    
    <Grid Name="BackupSize">
        <ScrollViewer Height="{Binding ElementName=BackupSize}" VerticalScrollBarVisibility="Hidden">
            <ItemsControl ItemsSource="{Binding backupListInfo}">
                
            </ItemsControl>
        </ScrollViewer>
    </Grid>
</UserControl>
