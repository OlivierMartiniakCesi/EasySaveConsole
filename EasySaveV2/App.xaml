<Application x:Class="EasySaveV2.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:EasySaveV2"
             xmlns:viewModel="clr-namespace:EasySaveV2.MVVM.ViewModels"
             xmlns:view="clr-namespace:EasySaveV2.MVVM.Views"
             StartupUri="MainWindow.xaml"
             >
    <Application.Resources>
        <ResourceDictionary x:Name="Dictionary">
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Language/DictionaryEnglish.xaml"/>
                <ResourceDictionary Source="/Style/MainDictonnary.xaml" />
                <ResourceDictionary Source="/Style/SettingsDictionary.xaml" />
                <ResourceDictionary Source="/Style/BackupDictionary.xaml"/>
                <ResourceDictionary Source="/Style/DashboardDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
